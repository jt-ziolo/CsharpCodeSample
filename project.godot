; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MyGameName"
config/version="0.1.0"
run/main_scene="res://assets/scenes/Main.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "C#", "Mobile")
run/max_fps=60
boot_splash/bg_color=Color(0.219501, 0.219501, 0.219501, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

GTweensGodotUpdater="*res://src/scripts/autoloaded/GTweensGodotUpdater.cs"

[debug]

canvas_items/max_fps=60
canvas_items/size/window_height_override=480
canvas_items/size/window_width_override=854
canvas_items/layer_5="enemies"
canvas_items/layer_4="destructables"
canvas_items/layer_1="world"
canvas_items/layer_3="weapons"
canvas_items/layer_2="player"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=854
window/size/window_height_override=480
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="MyGameName"

[editor_plugins]

enabled=PackedStringArray("res://addons/generate_godot_enums/plugin.cfg", "res://addons/imrp/plugin.cfg")

[gui]

theme/custom="res://assets/resources/default_theme.tres"

